###################################################################
#  Static positioning and control loop addresses                  #
###################################################################
record(stringin, "$(P)$(R)CH$(ADDR):FW_RBV")
{
   field(DTYP, "asynOctetRead")
   field(INP,  "@asyn($(PORT),0)LC400_FW")
   field(VAL,  "Unknown")
   field(SCAN, "I/O Intr")
}

record(ao,"$(P)$(R)CH$(ADDR):SP") {
    field(DESC, "Setpoint in cts")
    field(PREC,"3")
    field(DTYP, "asynFloat64")
    field(OUT,"@asyn($(PORT),$(ADDR))LC400_DIGITAL_POS")
}

record(ai,"$(P)$(R)CH$(ADDR):SP_RBV") {
    field(DESC, "Position in cts")
    field(PREC,"3")
    field(DTYP, "asynFloat64")
